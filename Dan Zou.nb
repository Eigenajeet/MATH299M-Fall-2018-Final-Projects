 RowBox[{
  RowBox[{
   RowBox[{"uRod", "[", 
    RowBox[{"A_", ",", "t_", ",", "u0_", ",", "v_"}], "]"}], ":=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", "eAu", "}"}], ",", 
     RowBox[{
      RowBox[{"eAu", "=", 
       RowBox[{
        RowBox[{"MatrixExp", "[", 
         RowBox[{"A", " ", "t"}], "]"}], ".", "u0"}]}], ";", 
      RowBox[{"Flatten", "[", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{
            RowBox[{"ColorData", "[", "\"\<TemperatureMap\>\"", "]"}], "[", 
            RowBox[{"Part", "[", 
             RowBox[{"eAu", ",", "part", ",", "1"}], "]"}], "]"}], ",", 
           RowBox[{"Rectangle", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"part", "-", "1"}], ",", "0"}], "}"}], ",", 
             RowBox[{"{", 
              RowBox[{"part", ",", 
               RowBox[{"v", "/", "20"}]}], "}"}]}], "]"}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"part", ",", "1", ",", "v"}], "}"}]}], "]"}], "]"}]}]}], 
    "]"}]}], ";"}]], "Input",
 InitializationCell->True,
