Cell[TextData[{
 "The derivative is linear, which means so is the fractional derivative! We\
\[CloseCurlyQuote]re going to want to take the derivative of more than just \
functions of the form ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["x", "n"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "0e4836b8-b172-46f4-9e63-e631059bc9ec"],
 ", so let\[CloseCurlyQuote]s write a function that does just that. Luckily, \
Mathematica makes rules like these really easy to define."
}], "Text",
 CellChangeTimes->{{3.753631920191412*^9, 3.753632013725473*^9}, 
   3.7536321954534197`*^9},ExpressionUUID->"15518972-a0af-408a-be82-\
cbf188cd0b36"],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{
     RowBox[{"d", "/", "fx"}], " ", 
     RowBox[{"(", 
      RowBox[{"f", " ", "+", " ", "g"}], ")"}]}], " ", "=", " ", 
    RowBox[{
     RowBox[{"df", "/", "dx"}], " ", "+", " ", 
     RowBox[{"dg", "/", "dx"}]}]}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"FD1", "[", 
     RowBox[{
      RowBox[{"f_", "+", "g_"}], ",", " ", "a_"}], "]"}], " ", ":=", " ", 
    RowBox[{
     RowBox[{"FD1", "[", 
      RowBox[{"f", ",", " ", "a"}], "]"}], " ", "+", " ", 
     RowBox[{"FD1", "[", 
      RowBox[{"g", ",", " ", "a"}], "]"}]}]}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{
      RowBox[{"d", "/", "dx"}], " ", 
      RowBox[{"(", 
       RowBox[{"c", "*", "f"}], ")"}]}], " ", "=", " ", 
     RowBox[{"c", " ", "*", " ", 
      RowBox[{"(", 
       RowBox[{"df", "/", "dx"}], ")"}]}]}], " ", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"FD1", "[", 
     RowBox[{
      RowBox[{"c_", "*", "f_"}], ",", " ", "a_"}], "]"}], " ", ":=", " ", 
    RowBox[{
     RowBox[{"c", "*", 
      RowBox[{"FD1", "[", 
       RowBox[{"f", ",", "a"}], "]"}]}], "/;", 
     RowBox[{"FreeQ", "[", 
      RowBox[{"c", ",", "x"}], "]"}]}]}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{
      RowBox[{"d", "/", "dx"}], " ", 
      RowBox[{"(", "c", ")"}]}], " ", "=", " ", "0"}], " ", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"FD1", "[", 
     RowBox[{"c_", ",", " ", "a_"}], "]"}], " ", ":=", " ", 
    RowBox[{"0", "/;", 
     RowBox[{"FreeQ", "[", 
      RowBox[{"c", ",", " ", "x"}], "]"}]}]}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"Our", " ", "definition", " ", "from", " ", 
     RowBox[{"earlier", "!"}]}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"FD1", "[", 
     RowBox[{
      RowBox[{"x", "^", "n_."}], ",", " ", "a_"}], "]"}], " ", ":=", " ", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"Gamma", "[", 
        RowBox[{"n", " ", "+", " ", "1"}], "]"}], "/", 
       RowBox[{"Gamma", "[", 
        RowBox[{"n", " ", "-", "a", " ", "+", " ", "1"}], "]"}]}], ")"}], "*", 
     SuperscriptBox["x", 
      RowBox[{"(", 
       RowBox[{"n", "-", "a"}], ")"}]]}]}]}]}]], "Input",
 CellChangeTimes->{{3.75363171093633*^9, 3.753631750692807*^9}, 
