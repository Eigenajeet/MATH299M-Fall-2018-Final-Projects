Cell["\<\
There are a lot of other details we could unpack here, but for now let\
\[CloseCurlyQuote]s get back to our original topic.\
\>", "Text",
 CellChangeTimes->{{3.753564328935652*^9, 
  3.7535643635998507`*^9}},ExpressionUUID->"c0a9fd28-5e5d-438d-a5f7-\
74734fce80b0"],

Cell[TextData[StyleBox["Using \[CapitalGamma](x)", "Chapter"]], "Text",
 CellChangeTimes->{{3.7535643685347347`*^9, 3.753564380975616*^9}, {
  3.753564797980934*^9, 
  3.7535647984439573`*^9}},ExpressionUUID->"ff217b35-19c3-454f-8b42-\
72dae497208c"],

Cell[TextData[{
 "Now, we can take",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox[
      SuperscriptBox["d", "a"], 
      SuperscriptBox["dx", "a"]], 
     SuperscriptBox["x", "n"]}], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"n", "!"}], 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"n", "-", "a"}], ")"}], "!"}]], 
     SuperscriptBox["x", 
      RowBox[{"n", "-", "a"}]]}]}], TraditionalForm]],ExpressionUUID->
  "fc0aea76-0537-4a8c-8840-559250e69863"],
 " and replace a with a real value \[Alpha]: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox[
      SuperscriptBox["d", "\[Alpha]"], 
      SuperscriptBox["dx", "\[Alpha]"]], 
     SuperscriptBox["x", "n"]}], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"\[CapitalGamma]", "(", 
       RowBox[{"n", "+", "1"}], ")"}], 
      RowBox[{
       RowBox[{"\[CapitalGamma]", "(", 
        RowBox[{"n", "-", "\[Alpha]", " ", "+", " ", "1"}], ")"}], "!"}]], 
     SuperscriptBox["x", 
      RowBox[{"n", "-", "\[Alpha]"}]]}]}], TraditionalForm]],ExpressionUUID->
  "538cb844-4d04-447f-a3bc-fdd2a29dabf4"],
 "\nMathematica doesn\[CloseCurlyQuote]t have this built in, so let\
\[CloseCurlyQuote]s define it (for functions ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["x", "n"], ")"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "b212b77f-4ed7-4b3d-bfa1-300eaee9a59f"],
 ":"
}], "Text",
 CellChangeTimes->{{3.753564393382452*^9, 3.753564431862035*^9}, {
  3.753564482861473*^9, 3.753564514682186*^9}, {3.753564786572686*^9, 
  3.753564826094783*^9}, {3.75356508231569*^9, 3.753565123202345*^9}, {
  3.753565171696566*^9, 3.75356517906008*^9}, {3.7535652122753468`*^9, 
  3.7535652163855333`*^9}},ExpressionUUID->"6836b6cc-5edf-4cb1-93ab-\
f7aa4dc76401"],

Cell[BoxData[
 RowBox[{
  RowBox[{"FDXN", "[", 
   RowBox[{"n_", ",", " ", "a_"}], "]"}], " ", ":=", " ", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{
     RowBox[{"Gamma", "[", 
      RowBox[{"n", " ", "+", " ", "1"}], "]"}], "/", 
     RowBox[{"Gamma", "[", 
      RowBox[{"n", " ", "-", "a", " ", "+", " ", "1"}], "]"}]}], ")"}], "*", 
   SuperscriptBox["x", 
    RowBox[{"(", 
     RowBox[{"n", "-", "a"}], ")"}]]}]}]], "Input",
 CellChangeTimes->{{3.7535651212394867`*^9, 3.7535651834957247`*^9}, {
  3.753565222106234*^9, 3.7535652503529177`*^9}, {3.753565288959198*^9, 
  3.75356529644654*^9}},ExpressionUUID->"09380f60-b775-4e35-964a-\
28cfd5ba6b21"],

